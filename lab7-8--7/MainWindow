import tkinter as tk
class MainApp(tk.Tk):
    def __init__(self, *arg, **kwarg):
        super(MainWindow, self).__init__(parent)
        self.parent # (присваивается parent, передаваемый извне);
        self.grid (row=0, column=0); # переменные, ассоциированные с виджетами
        self.principal = None
        self.rate = None
        self.years = None
        self.amount = None